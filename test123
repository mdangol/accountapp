node {
   def config = [:]
   config.num1 = 2
   config.num2 = 3
   config.num3 = 4
   config.num4 = 5
   
   println (config)
   stage('Addition') { 
     println (addNumbers(config))
   }
   //stage('Multiply') {
//     multiplyNumbers(config)
 //  }
  
}

def addNumbers(config) {
   // println (config.values())
   def sum = 0
    config.values().each { num -> (
        sum = sum + num
        )
    
    }
    return (sum)
}
